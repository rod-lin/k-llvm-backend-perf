REC-SPEC BubbleSort
SORTS
  Bool Nat NatList
OPS
  true : -> Bool
  false : -> Bool
  d0 : -> Nat                                           % zero
  d10 : -> Nat
  s : Nat -> Nat                                       % succesor
  lt : Nat Nat -> Bool                                 % less than 
  nil : -> NatList
  cons : Nat NatList -> NatList
  plus : Nat Nat -> Nat 
  times : Nat Nat -> Nat 
  rev : Nat -> NatList 
  fact : Nat -> Nat
VARS
  N M : Nat
  L : NatList
RULES
  d10 -> s(s(s(s(s(s(s(s(s(s(d0))))))))))
  lt(d0, s(N)) -> true
  lt(s(N), d0) -> false
  lt(N, N) -> false
  lt(s(N), s(M)) -> lt(N, M)
  plus(d0, N) -> N
  plus(s(N), M) -> s(plus(N, M))
  times(d0, N) -> d0
  times(s(N), M) -> plus(M, times(N, M))
  fact(d0) -> s(d0)
  fact(s(N)) -> times(s(N), fact(N))
  rev(s(N)) -> cons(s(N), rev(N))
  rev(d0) -> cons(d0, nil) 
  cons(N, cons(M, L)) -> cons(M, cons(N, L)) if lt(M, N) -><- true
END-SPEC

