REC-SPEC IntNat

% definition of integer numbers (see WADT'16 article) - Hubert Garavel 2016

SORTS

bool nat int

CONS

false : -> bool
true : -> bool

zero : -> nat
succ : nat -> nat

pos : nat -> int
neg : nat -> int

OPNS

not : bool -> bool
and : bool bool -> bool

even : nat -> bool
odd : nat -> bool
eq : nat nat -> bool            % "="
lt : nat nat -> bool            % "<"
pred : nat -> nat               % (partially-defined function)
add : nat nat -> nat            % "+"
sub : nat nat -> nat            % "-" (partially-defined function)
mult : nat nat -> nat           % "*"
div : nat nat -> nat            % (partially-defined function)
adiv : nat nat nat nat -> nat   % auxiliary function used to define "div"
cdiv : nat nat -> nat           % concise definition of "div" using cond. rules
mod : nat nat -> nat            % (partially-defined function)
amod : nat nat nat nat -> nat   % auxiliary function used to define "mod"
cmod : nat nat -> nat           % concise definition of "mod" using cond. rules

ieven : int -> bool
iodd : int -> bool
abs : int -> nat
ieq : int int -> bool   % "="
ilt : int int -> bool   % "<"
isucc : int -> int
ipred : int -> int
minus : int -> int      % unary "-"
iadd : int int -> int   % "+"
isub : int int -> int   % binary "-"
imult : int int -> int  % "*"
idiv : int int -> int   % (partially-defined function)
imod : int int -> int   % (partially-defined function)
irem : int int -> int   % (partially-defined function)

% --- check functions for non-trivial operators

check_div : nat nat nat -> bool
check_mod : nat nat nat -> bool
check_imult : int int int -> bool
check_idiv : int int int -> bool
check_imod : int int int -> bool
check_irem : int int int -> bool

VARS

b : bool
m n p q : nat
i j : int

% --- only for check functions

k : int

RULES

% --- boolean operators (well-known)

not (true) -> false
not (false) -> true

and (false, b) -> false
and (true, b) -> b

% --- natural operators (well-known)

even (zero) -> true
even (succ (n)) -> odd (n)

odd (zero) -> false
odd (succ (n)) -> even (n)

eq (zero, zero) -> true
eq (zero, succ (n)) -> false
eq (succ (n), zero) -> false 
eq (succ (m), succ (n)) -> eq (m, n)

lt (zero, zero) -> false
lt (zero, succ (n)) -> true
lt (succ (n), zero) -> false
lt (succ (m), succ (n)) -> lt (m, n)

% note: pred (n) is undefined if eq (n, zero)
pred (succ (n)) -> n

add (m, zero) -> m
add (m, succ (n)) -> add (succ (m), n)

% note: sub (m, n) is undefined if lt (m, n)
sub (m, zero) -> m
sub (succ (m), succ (n)) -> sub (m, n)

mult (m, zero) -> zero
mult (m, succ (n)) -> add (m, mult (m, n))

% note: div (m, n) is undefined if eq (n, zero)
div (m, succ (n)) -> adiv (m, succ (n), zero, succ (n))

adiv (zero, zero, q, n) -> succ (q)
adiv (zero, succ (p), q, n) -> q
adiv (succ (m), zero, q, n) -> adiv (succ (m), n, succ (q), n)
adiv (succ (m), succ (p), q, n) -> adiv (m, p, q, n)

% note: cdiv (m, n) is undefined if eq (n, zero)
cdiv (m, n) -> zero if and (not (eq (n, zero)), lt (m, n)) -><- true
cdiv (m, n) -> succ (cdiv (sub (m, n), n)) if and (not (eq (n, zero)), not (lt (m, n))) -><- true

% note: mod (m, n) is undefined if eq (n, zero)
mod (m, succ (n)) -> amod (m, succ (n), zero, succ (n))

amod (zero, zero, q, n) -> zero
amod (zero, succ (p), q, n) -> sub (n, succ (p))
amod (succ (m), zero, q, n) -> amod (succ (m), n, succ (q), n)
amod (succ (m), succ (p), q, n) -> amod (m, p, q, n)

% note: cmod (m, n) is undefined if eq (n, zero)
cmod (m, n) -> m if and (not (eq (n, zero)), lt (m, n)) -><- true
cmod (m, n) -> cmod (sub (m, n), n) if and (not (eq (n, zero)), not (lt (m, n))) -><- true

% --- integer operators

iodd (pos (n)) -> odd (n)
iodd (neg (n)) -> even (n)

ieven (pos (n)) -> even (n)
ieven (neg (n)) -> odd (n)

abs (pos (n)) -> n
abs (neg (n)) -> succ (n)

ieq (pos (m), pos (n)) -> eq (m, n)
ieq (pos (m), neg (n)) -> false
ieq (neg (m), pos (n)) -> false
ieq (neg (m), neg (n)) -> eq (m, n)

ilt (pos (m), pos (n)) -> lt (m, n)
ilt (pos (m), neg (n)) -> false
ilt (neg (m), pos (n)) -> true
ilt (neg (m), neg (n)) -> lt (n, m)

isucc (pos (n)) -> pos (succ (n))
isucc (neg (zero)) -> pos (zero)
isucc (neg (succ (n))) -> neg (n)

ipred (pos (zero)) -> neg (zero)
ipred (pos (succ (n))) -> pos (n)
ipred (neg (n)) -> neg (succ (n))

minus (pos (zero)) -> pos (zero)
minus (pos (succ (n))) -> neg (n)
minus (neg (n)) -> pos (succ (n))

iadd (pos (zero), i) -> i
iadd (pos (succ (n)), i) -> iadd (pos (n), isucc (i))
iadd (neg (zero), i) -> ipred (i)
iadd (neg (succ (n)), i) -> iadd (neg (n), ipred (i))

% AUTRE PISTE POSSIBLE ?
% jadd (pos (m), pos (n)) -> pos (add (m, n))
% ...

isub (i, j) -> iadd (i, minus (j))

imult (pos (m), pos (n)) -> pos (mult (m, n))
imult (pos (m), neg (n)) -> minus (pos (mult (m, succ (n))))
imult (neg (m), pos (n)) -> minus (pos (mult (succ (m), n)))
imult (neg (m), neg (n)) -> pos (mult (succ (m), succ (n)))

% note: idiv (i, j) is undefined if ieq (j, zero)
idiv (pos (m), pos (n)) -> pos (div (m, n))
idiv (pos (m), neg (n)) -> minus (pos (div (m, succ (n))))
idiv (neg (m), pos (n)) -> minus (pos (div (succ (m), n)))
idiv (neg (m), neg (n)) -> pos (div (succ (m), succ (n)))

% note: imod (i, j) is undefined if ieq (j, zero)
imod (pos (m), pos (n)) -> pos (mod (m, n))
imod (pos (m), neg (n)) -> minus (pos (mod (sub (succ (n), mod (m, succ (n))), succ (n))))
imod (neg (m), pos (n)) -> pos (mod (sub (n, mod (succ (m), n)), n))
imod (neg (m), neg (n)) -> minus (pos (mod (succ (m), succ (n))))

% note: irem (i, j) is undefined if ieq (j, zero)
irem (pos (m), j) -> pos (mod (m, abs (j)))
irem (neg (m), j) -> minus (pos (mod (succ (m), abs (j))))

% --- check functions

check_div (m, n, q) -> and (eq (div (m, n), q), eq (cdiv (m, n), q))
check_mod (m, n, q) -> and (eq (mod (m, n), q), eq (cmod (m, n), q))
check_imult (i, j, k) -> ieq (imult (i, j), k)
check_idiv (i, j, k) -> ieq (idiv (i, j), k)
check_imod (i, j, k) -> ieq (imod (i, j), k)
check_irem (i, j, k) -> ieq (irem (i, j), k)

EVAL

